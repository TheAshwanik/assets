changeTab('encrypt');function changeTab(activeTab){document.querySelectorAll('.tabs li').forEach(function(li){if(li.children[0].dataset.tab===activeTab){li.classList.add('is-active');}else{li.classList.remove('is-active');}});document.querySelectorAll('.tab').forEach(function(tab){tab.style.display=tab.id===activeTab?'block':'none';});}